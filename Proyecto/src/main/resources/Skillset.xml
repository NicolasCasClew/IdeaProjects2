<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="DBs.Skillset">
        <table name="SKILLSET" schema="SYSTEM"/>
        <attributes>
            <id name="ssId">
                <column name="SS_ID"/>
            </id>
            <basic name="skillset">
                <column name="SKILLSET"/>
            </basic>
            <basic name="skill">
                <column name="SKILL"/>
            </basic>
            <basic name="lvl">
                <column name="LVL"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
